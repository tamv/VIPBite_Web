<div class="whiteBG">
	<div class="container">
		<div class="row">
			<div class="col-md-7">
				<h1>Register</h1>
			</div>
			<div class="col-md-5">
				<div class="searchBG">
					<h2>Already a membber? <a href="/login">Login Here!</a></h2>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="row">
				<div class="col-md-7">
					<section clas="registerForm">
						<form>
							<input class="registrationInput" id="firstName" maxlength="30" name="firstName" size="30" type="text" placeholder="First Name" />
							<input class="registrationInput" id="lastName" maxlength="30" name="lastName" size="30" type="text" placeholder="Last Name" />
							<div class="inputLabel">
								Select Title:
							</div>
							<select class="registrationInput" id="userTitle" name="userTitle"><option value="MR.">MR.</option>
								<option value="MRs.">MRs.</option>
								<option value="MS.">MS.</option>
							</select>
							<input class="registrationInput" id="email" maxlength="30" name="email" size="30" type="email" placeholder="Email" />
							<input class="registrationInput" id="password" maxlength="20" name="password" size="30" type="password" placeholder="Password" />
							<input class="registrationInput" id="cfm_password" maxlength="20" name="cfm_password" size="30" type="password" placeholder="Confirm Password" />
							<h4 class="">Payment &amp; Billing Information</h4>
							<div class="inputLabel">
								Length:
							</div>
							<div class="clear"></div>
							<input id="memberType_6months" class="form-radio" name="memberType" type="radio" value="6months" /><label for="memberType_6months">6 Months</label>
							<input id="memberType_12months" class="form-radio" name="memberType" type="radio" value="12months" />	<label for="memberType_12months">12 Months</label>
							<input class="registrationInput" id="noc_firstname" maxlength="30" name="noc_firstname" placeholder="First Name" size="30" type="text" /></td>
							<input class="registrationInput" id="noc_lastname" maxlength="30" name="noc_lastname" placeholder="Last Name" size="30" type="text" /></td>
							<div class="inputLabel">
								Select Card:
							</div>
							<select class="registrationInput" id="cardtype" name="cardtype"><option value="VISA">VISA</option>
								<option value="MASTERCARD">MASTERCARD</option>
								<option value="AMERICANEXP">AMERICANEXP</option>
							</select>
							<input class="registrationInput" id="cardno" maxlength="20" name="cardno" size="30" type="text" placeholder="Card Number" />
							<input class="registrationInput" id="exp_month" maxlength="2" name="exp_month" placeholder="MM" size="2" type="text" />
							<input class="registrationInput" id="exp_year" maxlength="4" name="exp_year" placeholder="YYYY" size="4" type="text" />
						
							<input class="registrationInput" id="cvv" maxlength="3" name="cvv" size="30" type="password" placeholder="CVV" />
					
							<input class="registrationInput" id="unitno" maxlength="60" name="unitno" size="30" type="text" placeholder="Unit Number" />
							<input class="registrationInput" id="address" maxlength="60" name="address" size="30" type="text" placeholder="Address" />
						
							<input class="registrationInput" id="city" maxlength="20" name="city" size="30" type="text" placeholder="City" />
							<div class="inputLabel">
								Select Province:
							</div>
							<select class="registrationInput" id="prov" name="prov">
								<option value="AB">AB</option>
								<option value="BC">BC</option></select>
					
							<input class="registrationInput" id="postal" maxlength="7" name="postal" size="30" type="text" placeholder="Postal Code" />
							<input class="registrationInput" id="phone" maxlength="11" name="phone" size="30" type="text" placeholder="Phone Number" />
							<input class="submit" name="commit" type="submit" value="Register" /></span>
						</form>
					</section>
				</div>
				<div class="col-md-5">
					<div class="sideBar">
						<h2>How It Works</h2>
						<ul>
							<li>Eat at one of many VIPbite partner restaurants</li>
							<li>Book your table & save big by being a VIPbite member</li>
							<li>Enjoy your meal, show your VIPbite app, get your discount!</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<script>
	$(window).bind("load", function() {
			$(window).resize(function () {
				$("#gmContainer").height($("#restaurantSearch").height())
			})
			$("#howItWork").hide();
			$("#staticHeader").addClass("subHead");
			initialize_withLoc()
		})
</script>